---------------------------------------------------------------------------------------------------------------------
_G.openEgg = true
_G.selectedEgg = "Pyecy Village"
local eggsTable = {}

for i,v in pairs(game:GetService("Workspace")["_EGGS"]:GetChildren()) do
	table.insert(eggsTable, v.Name)
end

EggsTab:AddDropdown({
	Name = "Select an Egg",
	Default = "Pyecy Village",
	Options = eggsTable,
	Callback = function(Value)
		_G.selectedEgg = Value
	end    
})

function OpenEgg()
	while _G.openEgg == true do
		game:GetService("ReplicatedStorage").Remotes.Server:FireServer({"BuyHeroes",_G.selectedEgg})
		wait(0.01)
	 end
end

EggsTab:AddToggle({
	Name = "Auto Buy Egg",
	Default = false,
	Callback = function(Value)
		_G.openEgg = Value
		OpenEgg()
	end 
})

local Section = EggsTab:AddSection({
	Name = "Teleport To Eggs"
})

_G.selectedEggTP = ""

EggsTab:AddDropdown({
	Name = "Please choose an egg to teleport to",
	Default = "...",
	Options = eggsTable,
	Callback = function(Value)
		_G.selectedEggTP = Value
	end    
})

EggsTab:AddButton({
	Name = "Teleport To Egg",
	Callback = function()
      	for i,v in pairs(game:GetService("Workspace")["_EGGS"][_G.selectedEggTP]:GetChildren()) do
			if v.Name == "Range" and v:IsA('Part') then
				game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.CFrame
			end
		end
  	end    
})


