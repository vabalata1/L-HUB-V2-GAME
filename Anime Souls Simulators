-- Labels

FarmTab:AddLabel("CHOOSE A MOB:")

----------------------------------------------------PYECY VILLAGE---------------------------------------------------------
FarmTab:AddLabel("Pyecy Village:")

_G.selectedMobPyecy = ""

FarmTab:AddDropdown({
	Name = "Selected a Mob",
	Default = "None",
	Options = {"crocodile", "mihawk", "aokiji", "kizaru"},
	Callback = function(Value)
		_G.selectedMobPyecy = Value
	end
})

function autoFarmPyecy()
    loadstring(game:HttpGet(('https://pastebin.com/raw/cUJte4SZ')))()
end

FarmTab:AddToggle({
	Name = "Enable AutoFarm",
	Default = false,
	Callback = function(Value)
        getgenv().bandit = Value
        autoFarmPyecy()
    end
})

