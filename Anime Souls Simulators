function  DekuOFA()
	while _G.attackMob == true do

		for i,v in pairs(game:GetService("Workspace")["_ENEMIES"]["Academy Zone"]:GetChildren()) do
			if v.Name == "deku_ofa" and v:IsA('Model') then
				for i,v in pairs(v:GetChildren()) do
					if v.Name == "HumanoidRootPart" and v:IsA('Part') then
						game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.CFrame
					end
				end
			end
		end

		game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-30378.1523, 18.9276733, 583.5495, -1, 0, 0, 0, 1, 0, 0, 0, -1)
		game:GetService("ReplicatedStorage").Remotes.Server:FireServer({"Hit",workspace._ENEMIES["Academy Zone"].deku_ofa})
		wait(0.05)
	end
end

function Garou()
	while _G.attackMob == true do

		for i,v in pairs(game:GetService("Workspace")["_ENEMIES"]["XYZ Province"]:GetChildren()) do
