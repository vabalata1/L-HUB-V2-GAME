function autoBuyPassive()
	while _G.autoBuyPassive == true do
		game:GetService("ReplicatedStorage").Remotes.Server:FireServer({"PassiveRoll"})
	wait(0.01)
end
end

function autoBuyAuras()
	while _G.autoBuyAuras == true do
		game:GetService("ReplicatedStorage").Remotes.Server:FireServer({"Auras"})
	wait(0.01)
end
end 

