-------------------------------------------GRIMORIES--------------------------------------------- 
_G.autoGrim = true
_G.whatGrim = ""

AutomaticsTab:AddDropdown({
	Name = "Selected a Type",
	Default = "None",
	Options = {"Coins", "Robux"},
	Callback = function(Value)
		_G.whatGrim = Value
	end
})

function autoGrim()
	while _G.autoGrim == true do
		game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-39546.6133, 7.75431013, 599.622009, 2.21133232e-05, -0.946120381, 0.323815167, 1, 2.21133232e-05, -3.68058681e-06, -3.68058681e-06, 0.323815167, 0.946120381)
		if _G.whatGrim == "Coins" then
			game:GetService("ReplicatedStorage").Remotes.Server:FireServer({"Grimoire","Normal"})
		end
		if _G.whatGrim == "Robux" then
			game:GetService("ReplicatedStorage").Remotes.Server:FireServer({"Grimoire","Robux"})
		end
	wait()
	end
end

AutomaticsTab:AddToggle({
	Name = "Auto Buy Grimoires",
	Default = false,
	Callback = function(Value)
		_G.autoGrim = Value
		autoGrim()
	end    
})

