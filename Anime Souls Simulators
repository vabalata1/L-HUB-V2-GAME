_G.selectedPotion = ""
_G.autoUsePotion = true

PotionsTab:AddDropdown({
	Name = "Select a Potion",
	Default = "...",
	Options = {"energy", "lucky", "damage", "souls"},
	Callback = function(Value)
		_G.selectedPotion = Value
	end
})

function UseSelectedPotion()
	while _G.autoUsePotion == true do
		game:GetService("ReplicatedStorage").Remotes.Server:FireServer({"Potions",_G.selectedPotion})
		wait(0.01)
	end
end

PotionsTab:AddToggle({
	Name = "Auto Use Potion",
	Default = false,
	Callback = function(Value)
		_G.autoUsePotion = Value
		UseSelectedPotion()
	end   
})

PotionsTab:AddLabel('If you wanna use multiple ones:')

