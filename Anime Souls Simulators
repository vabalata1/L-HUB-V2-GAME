	Increment = 1,
	ValueName = "seconds",
	Callback = function(Value)
		_G.webhookCooldown = Value
	end    
})

function sendWebhook()
	loadstring(game:HttpGet(('https://pastebin.com/raw/UpYrQc0J')))()
end


WBTab:AddToggle({
	Name = "Auto Send Webhook",
	Default = false,
	Callback = function(Value)
		_G.webhookOn = Value
		sendWebhook()
	end    
})

WBTab:AddLabel('WEBHOOKS ARE LAGGY AND MIGHT NOT WORK FOR ALL.')

------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
_G.meteorIsland = 0
_G.autofarmMeteor = true

function getMeteorIsland()
    for i,v in pairs(game:GetService("Workspace")["_METEORS"]:GetChildren()) do
        if v:IsA('Model') then
            for i, v in pairs(v:GetChildren()) do
                if v.Name == "_stats" and v:IsA('Folder') then
                    for i,v in pairs(v:GetChildren()) do
                        if v.Name == "island" then
                            _G.meteorIsland = v.Value
                        end
                    end
                end
            end
        end
    end
end

function tpToMeteor()
	for i,v in pairs(game:GetService("Workspace")["_METEORS"]:GetChildren()) do
		if v:IsA('Model') then
			for i, v in pairs(v:GetChildren()) do
				if v.Name == "root" then
					while _G.autofarmMeteor == true do
						game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.CFrame
						wait()
					end
				end
			end
		end
	end
end

MeteorsTab:AddToggle({
	Name = "Auto Farm Meteor",
	Default = false,
	Callback = function(Value)
		_G.autofarmMeteor = Value
		while _G.autofarmMeteor == true do
		getMeteorIsland()
		game:GetService("ReplicatedStorage").Remotes.Server:FireServer({"Teleport",_G.meteorIsland})
		tpToMeteor()
		wait(10)
		end
	end    
})
